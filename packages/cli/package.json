{
  "name": "@nextsparkjs/cli",
  "version": "0.1.0-beta.91",
  "description": "NextSpark CLI - Complete development toolkit",
  "type": "module",
  "bin": {
    "nextspark": "./bin/nextspark.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "bin",
    "templates"
  ],
  "scripts": {
    "build": "tsup src/cli.ts --format esm --dts --outDir dist",
    "dev": "tsup src/cli.ts --format esm --watch"
  },
  "dependencies": {
    "@inquirer/prompts": "^7.2.0",
    "@nextsparkjs/core": "^0.1.0-beta.75",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "dotenv": "^17.2.2",
    "fs-extra": "^11.2.0",
    "ora": "^8.0.0",
    "tar": "^7.0.0"
  },
  "devDependencies": {
    "@nextsparkjs/core": "workspace:*",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.0.0",
    "@types/tar": "^6.1.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "@nextsparkjs/ai-workflow": "^0.1.0"
  },
  "peerDependenciesMeta": {
    "@nextsparkjs/ai-workflow": {
      "optional": true
    }
  },
  "keywords": [
    "nextspark",
    "cli",
    "nextjs",
    "saas",
    "boilerplate"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/NextSpark-js/nextspark.git",
